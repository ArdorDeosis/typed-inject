{
  "name": "typed-inject",
  "version": "0.0.0",
  "description": "Type safe dependency injection framework for TypeScript",
  "main": "src/index.js",
  "typings": "src/index.d.ts",
  "scripts": {
    "start": "tsc -b -w",
    "prebuild": "rimraf \"+(src|test)/**/*+(.map|.js|.d.ts)\"",
    "build": "tsc -b",
    "pretest": "npm run build",
    "test": "nyc --check-coverage --reporter=html --report-dir=reports/coverage --lines 90 --functions 95 --branches 80 npm run mocha",
    "mocha": "mocha \"test/helpers/**/*.js\" \"test/unit/**/*.js\" && mocha --timeout 20000 \"test/helpers/**/*.js\" \"test/integration/**/*.js\" "
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nicojs/typed-inject.git"
  },
  "engines": {
    "node": ">=6"
  },
  "keywords": [
    "typescript",
    "dependency-injection",
    "IoC",
    "di",
    "inject",
    "type-safe"
  ],
  "publishConfig": {
    "access": "public"
  },
  "author": "Nico Jansen <jansennico@gmail.com>",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/nicojs/typed-inject/issues"
  },
  "homepage": "https://github.com/nicojs/typed-inject/#readme",
  "devDependencies": {
    "@types/chai": "^4.1.7",
    "@types/mocha": "^5.2.5",
    "@types/node": "^10.12.18",
    "chai": "^4.2.0",
    "mocha": "^5.2.0",
    "nyc": "^13.1.0",
    "rimraf": "^2.6.3",
    "source-map-support": "^0.5.10",
    "tslint": "^5.12.1",
    "typescript": "^3.2.4"
  }
}
